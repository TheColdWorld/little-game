<Window x:Class="little_game.client.wpf.starter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:little_game.client.wpf"
        mc:Ignorable="d"
        Title="starter" Height="140" Width="150">
    <StackPanel Orientation="Vertical">
        <TextBlock Text="服务端的IPv4地址" Height="20"/>
        <TextBox x:Name="IPv4" Height="20"/>
        <TextBlock Text="开放端口" Height="20"/>
        <TextBox x:Name="port" Height="20"/>
        <Button Content="确定" Height="20" Width="30" Click="Button_Click" VerticalAlignment="Center" HorizontalAlignment="Right" HorizontalContentAlignment="Right"/>
    </StackPanel>
</Window>
